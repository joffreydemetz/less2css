{
    "name": "jdz/less2css",
    "type": "library",
    "description": "Parse/Compress Less to CSS",
    "keywords": ["less", "css", "php", "stylesheet", "less.js", "lesscss"],
    "homepage": "https://github.com/joffreydemetz/less2css/",
    "version": "2.0.1",
    "license": "MIT",
    
    "authors": [
        {
            "name": "Joffrey Demetz",
            "email": "joffrey.demetz@gmail.com",
            "homepage": "https://joffreydemetz.com",
            "role": "Lead"
        }
    ],

    "require": {
        "php": "^7.1.3"
    },
    
    "suggest": {
        "mishal/iless": "Needed to support the ILess parser ^2.2",
        "oyejorge/less.php": "Needed to support the LessC parser 1.7.0",
        "websharks/css-minifier": "Needed to support the WebSharks minifier 150820"
    },
    
    "conflict": {
        "mishal/iless": "<2.2 >2.3",
        "oyejorge/less.php": "<1.7.0 >1.7.0",
        "websharks/css-minifier": "<150820 >150820"
    },
    
    "autoload": {
        "psr-4": {
            "JDZ\\Less2Css\\": "src"
        }
    }
}
